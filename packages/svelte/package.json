{
  "name": "@aural-ui/svelte",
  "version": "1.0.0",
  "description": "Aural UI - Svelte components, actions, and stores",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "svelte": "dist/index.js",
  "exports": {
    ".": {
      "svelte": "./dist/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && svelte-package",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "svelte",
    "ui",
    "components",
    "design-system",
    "aural-ui"
  ],
  "author": "Workshop BAI Team",
  "license": "MIT",
  "peerDependencies": {
    "svelte": ">=3.0.0 || >=4.0.0"
  },
  "dependencies": {
    "@aural-ui/core": "workspace:*"
  },
  "devDependencies": {
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "svelte": "^4.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  }
}
