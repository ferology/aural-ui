{
  "name": "@aural-ui/vue",
  "version": "1.0.0",
  "description": "Aural UI - Vue 3 components and composables",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "vue",
    "vue3",
    "ui",
    "components",
    "design-system",
    "aural-ui"
  ],
  "author": "Workshop BAI Team",
  "license": "MIT",
  "peerDependencies": {
    "vue": ">=3.0.0"
  },
  "dependencies": {
    "@aural-ui/core": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.7.0",
    "vue-tsc": "^1.8.0"
  }
}
